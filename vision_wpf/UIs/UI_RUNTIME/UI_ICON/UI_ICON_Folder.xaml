<local:UI_ICON_ModuleItem x:Class="ns_vision.UI_ICON_Folder"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ns_vision"
             mc:Ignorable="d" 
             Width="64" Height="68" Margin="4, 8, 4, 8">
    <Grid>
        <DockPanel>
            <TextBlock x:Name="m_title" DockPanel.Dock="Bottom" HorizontalAlignment="Center" VerticalAlignment="Center" Foreground="White" FontSize="16"> asdf</TextBlock>
            <Grid DockPanel.Dock="Top" Margin="2, 2, 2, 0">
                <Rectangle Margin="2,2,8,8" Fill="#FFACD69B" Stroke="White">
                </Rectangle>
                <Rectangle Margin="5,5,5,5" Fill="#FFACD69B" Stroke="White">
                </Rectangle>
                <Rectangle Margin="8,8,2,2" Fill="#FFACD69B" Stroke="White">
                </Rectangle>
            </Grid>
        </DockPanel>
        <Rectangle x:Name="m_mask" Fill="#38263385"></Rectangle>
    </Grid>
</local:UI_ICON_ModuleItem>
